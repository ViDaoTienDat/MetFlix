{
  "expo": {
    "name": "MetFlix",
    "slug": "MetFlix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/metflix.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoMetFlix.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/metflix.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vidat.MetFlix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "54cdd3ea-dad5-4585-851a-57273b817479"
      }
    }
  }
}
